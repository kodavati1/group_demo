<% unless @types.nil? %>
  <table class="mrgn_lft_6">
    <% @types.each do |type| %>
      <tr>
        <%#= content_tag_for(:tr, keyword) do %>
          <td>
            <span data-toggle='tooltip' data-placement='left' title=<%= type.type %>>
                <%= truncate(type.type, length: 30) %>
            </span>
          </td>
          <td>&nbsp;&nbsp;</td>
          <%# unless current_party.published? %>
            <td> <%= link_to '', edit_type_path(type),  class: "glyphicon glyphicon-pencil", remote: true %></td>
            <td><%= link_to '', type_path(type),  class: "glyphicon glyphicon-remove", remote: 'true', method: :delete, data: {confirm: 'Are you sure?'} %></td>
          <%# else %>
          <%# end %>
        <%#end %>
        <td><%= link_to 'show', type %> </td>
        <td><%= link_to 'edit', edit_type_path(type) %></td>
      </tr>
    <% end %>
  </table>
<% end %>